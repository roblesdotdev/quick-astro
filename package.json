    "lint": "eslint .",
    "prepare": "husky",
    "validate": "run-p check lint format"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.2",
    "astro": "^4.14.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "eslint-plugin-astro": "^1.2.3",
    "husky": "^9.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.6"
  },
  "engines": {
    "node": ">=20.14.0",
    "pnpm": ">=9.7.1",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@9.7.1"
