{
  "name": "quick-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro build && astro preview",
    "astro": "astro",
    "check": "astro check",
    "prettier": "prettier \"**/*.+(js|jsx|astro|css|ts|tsx|cjs|mjs)\"",
    "format": "pnpm prettier --list-different",
